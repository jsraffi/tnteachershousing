@model IEnumerable<tnteachershousing.Models.ListUsers>

@{
    ViewBag.Title = "getAllUsers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1 class="pageheader">User List</h1>


<div class="container col-md-10">
    <div class="table-responsive">
        <table class="table table-bordered table-hover fontadmintable">
            <tr style="background-color:lightblue;">
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.ActionLink("Edit user", "Edit", new {  id=item.UserName })
                        
                    </td>
                </tr>
            }

        </table>
        </div>
    </div>
